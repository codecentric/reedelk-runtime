{
  "id": "3c5b1848-23b8-48a3-b674-2c230837418d",
  "title": "Module Health",
  "description": "Health flow Flow",
  "flow": [
    {
      "implementor": "de.codecentric.reedelk.rest.component.RESTListener",
      "description": "REST Listener",
      "configuration": {"ref": "8d689312-0cc5-4e0b-aec6-f17025627aa1"},
      "path": "/api/health",
      "method": "GET",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200,
        "headers": {"Access-Control-Allow-Origin": "#['*']"}
      },
      "errorResponse": {
        "body": "#[error.getMessage()]",
        "status": 500,
        "headers": {"content-type": "application/json", "Access-Control-Allow-Origin": "#['*']"}
      }
    },
    {
      "implementor": "de.codecentric.reedelk.admin.console.SystemHealth",
      "description": "System health"
    }
  ]
}
